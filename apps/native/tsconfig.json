{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"baseUrl": ".",
		"target": "esnext",
		"module": "commonjs",
		"jsx": "react-native",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"types": ["jest", "react", "react-native"],
		"paths": {
			"@api-types/*": ["../../apps/api/dist/src/types/*"]
		}
	}
}
